{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./en_API Postman Test_strings.csv",
          "file_format": "csv",
          "tag": "1",
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "language": "en"
        },
        {
          "path": "./en.csv",
          "file_format": "csv",
          "tag": "2",
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./<language>1.csv",
          "file_format": "csv",
          "tag": "1",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>2.csv",
          "file_format": "csv",
          "tag": "2",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}