{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./en_API Postman Test_strings.csv",
          "file_format": "csv",
          "tag": "app:language.csv1",
          "language": "en"
        },
        {
          "path": "./en.csv",
          "file_format": "csv",
          "tag": "app:language.csv2",
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./<language>_1.csv",
          "file_format": "csv",
          "tag": "app:language.csv",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>_2.csv",
          "file_format": "csv",
          "tag": "app:language.csv",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}